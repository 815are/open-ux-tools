{
    "name": "@sap-ux/fpm-cli",
    "version": "0.8.0",
    "description": "Simple example CLI creating FPM enable example projects.",
    "license": "SEE LICENSE IN LICENSE",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "pnpm clean && tsc",
        "watch": "tsc --watch",
        "clean": "rimraf dist coverage .tmp",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "dependencies": {
        "@sap-ux/fe-fpm-writer": "workspace:*",
        "@sap-ux/ui5-application-writer": "workspace:*"
    },
    "files": [
        "dist",
        "LICENSE"
    ],
    "engines": {
        "pnpm": ">=6.26.1",
        "node": ">=12.22.5 < 13.0.0 || >= 14.16.0 < 15.0.0 || >=16.1.0 < 17.0.0"
    }
}
