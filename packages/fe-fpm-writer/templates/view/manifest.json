{
    "sap.ui5": {
        <% if (locals.eventHandler) { %> 
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "<%- ns %>.<%- name %>"
                    }
                }
            }
        },
        <% } %>
        "routing": {
            "targets": {
                "<%- target %>": {
                    "options": {
                        "settings": {
                            "views": {
                                "paths": [
                                    {
                                        "key": "<%- key %>",
                                        "label": "<%- label %>",
                                        "template": "<%- ns %>.<%- name %>"
                                    }
                                ]
                            }
                        }
                    }
                }
            }
        }
    }
}