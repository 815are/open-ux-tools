<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified">
    <Table id="<%- name %>">
        <headerToolbar>
            <OverflowToolbar>
                <content>
                    <Title text="Header Title" level="H2" />
                    <ToolbarSpacer />
                </content>
            </OverflowToolbar>
        </headerToolbar>
        <infoToolbar>
            <OverflowToolbar>
                <Label text="Toolbar Label" />
            </OverflowToolbar>
        </infoToolbar>
        <columns>
            <Column>
                <Text text="Column" />
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <%- content %>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>
